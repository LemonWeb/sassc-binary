{
    "name": "lemonweb/sassc-binary",
    "type": "library",
    "description": "A wrapper package to compile the sassc binary from hcatlin's source",
    "keywords": ["CSS", "SASS", "SCSS"],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sass/libsass",
                "version": "3.1.0",
                "source": {
                    "url": "https://github.com/sass/libsass",
                    "type": "git",
                    "reference": "3.1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sass/sassc",
                "version": "3.1.0",
                "source": {
                    "url": "https://github.com/sass/sassc",
                    "type": "git",
                    "reference": "3.1.0"
                }
            }
        }
    ],
    "require-dev": {
        "sass/libsass": "3.1.0",
        "sass/sassc": "3.1.0"
    },
    "bin": [
        "sassc"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
